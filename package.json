{
  "name": "@adufr/prettier-config",
  "version": "1.0.1",
  "packageManager": "pnpm@10.18.2",
  "description": "Prettier config for @adufr",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/adufr/prettier-config#readme",
  "bugs": {
    "url": "https://github.com/adufr/prettier-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adufr/prettier-config.git"
  },
  "author": "Arthur Dufour <dufourarthur.perso@gmail.com>",
  "files": [
    "index.d.ts",
    "index.js"
  ],
  "types": "index.d.ts",
  "exports": {
    ".": "./index.js",
    "./package.json": "./package.json"
  },
